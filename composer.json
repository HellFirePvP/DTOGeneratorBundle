{
    "name": "hellfirepvp/dto-generator-bundle",
    "description": "This bundle provides utility to create DTO objects from POP objects.",
    "version": "0.0.1",
    "keywords": ["dto"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "HellFirePvP",
            "email": "dev.hellfire@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.5",
        "doctrine/annotations": "^1.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/validator": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "HellFirePvP\\Bundle\\DTOGeneratorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HellFirePvP\\Bundle\\DTOGeneratorBundle\\Tests\\": "Tests/"
        }
    }
}
